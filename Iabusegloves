tab.newButton("Oven", "Oven abillity", function() 
fireclickdetector(workspace.Lobby.GloveStands.Oven.ClickDetector)
task.wait(0.2)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local rootPart = character:FindFirstChild("HumanoidRootPart")

if rootPart then
    local closestPlayer, closestDistance = nil, math.huge

    for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local distance = (otherPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
            if distance < closestDistance then
                closestDistance = distance
                closestPlayer = otherPlayer
            end
        end
    end

    if closestPlayer then
        local targetRoot = closestPlayer.Character.HumanoidRootPart
        local args = {
            [1] = CFrame.new(targetRoot.Position)
        }

        game:GetService("ReplicatedStorage"):WaitForChild("GeneralAbility"):FireServer(unpack(args))
    end
end
end)

